{% extends "layout.jinja2" %}

{% block content %}
<div class="content">
  <h1><span class="font-semi-bold">Pyramid</span> <span class="smaller">Alchemy scaffold</span></h1>
  <p class="lead">Welcome to <span class="font-normal">{{project}}</span>, an&nbsp;application generated&nbsp;by<br>the <span class="font-normal">Pyramid Web Framework 1.7.3</span>.</p>
</div>
<div>
              <h2>Hello, {{ entry.username) }}</h2>
              <h3>Profile</h3>
              <p>{{ entry.first_name }} {{ entry.last_name}}</p>
              <p>{{ entry.email }} </p>
              <p>Favorite food: {{ entry.food }}</p>
            <hr>
          {%endfor%}
</div>
{% endblock content %}
